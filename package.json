{
  "name": "albi-gpt",
  "version": "1.0.70",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-dev": "ng serve --configuration=development --host 0.0.0.0",
    "start-prod": "ng serve --configuration=production --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "upgrade-cli": "ng update @angular/core@20 @angular/cli@20",
    "build:prod": "ng build --configuration=production --base-href https://albi23.github.io/Albi-GPT/",
    "build:prod-local": "ng build --configuration=production --base-href='/'",
    "postbuild:prod-local": "node src/post-build-script/fix-local-build.js",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.15",
    "@angular/common": "^20.3.15",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.15",
    "@angular/forms": "^20.3.15",
    "@angular/platform-browser": "^20.3.15",
    "@angular/platform-browser-dynamic": "^20.3.15",
    "@angular/router": "^20.3.15",
    "clipboard": "^2.0.11",
    "marked": "^16.0.0",
    "ngx-markdown": "^20.0.0",
    "prismjs": "1.30.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "~20.7.0",
    "@angular-eslint/eslint-plugin": "~20.7.0",
    "@angular-eslint/eslint-plugin-template": "~20.7.0",
    "@angular-eslint/schematics": "~20.7.0",
    "@angular-eslint/template-parser": "~20.7.0",
    "@angular/build": "^20.3.13",
    "@angular/cli": "~20.3.13",
    "@angular/compiler-cli": "^20.3.15",
    "@types/jasmine": "~5.1.4",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "eslint": "^8.56.0",
    "jasmine-core": "~4.6.1",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "replace-in-file": "~7.1.0",
    "typescript": "~5.9.3"
  }
}
